          },
          "properties": {
            "width": 20
          }
        }
      },
      "components": {
        "icon": {
          "states": {
            "default": {
              "styles": {
                "transform": "rotateZ(180deg)",
                "transition": "left 150ms, transform 150ms"
              },
              "properties": {
                "scale": true,
                "width": 24,
                "height": 24
              }
            },
            "collapsed": {
              "styles": {
                "transform": "rotateZ(0deg)"
              }
