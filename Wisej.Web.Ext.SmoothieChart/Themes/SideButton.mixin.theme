{
  "name": "SideButton",
  "images": {
  },
  "colors": {
  },
  "appearances": {
    "side-button": {
      "states": {
        "default": {
          "styles": {
            "transition": "width 150ms, transform 150ms"
          },
          "properties": {
            "height": 40
          }
        },
        "left": {
          "styles": {
            "radius": [ 0, 40, 40, 0 ]
          }
        },
        "right": {
          "styles": {
            "radius": [ 40, 0, 0, 40 ]
          }
        },
        "hovered": {
          "properties": {
            "cursor": "pointer"
          }
        },
        "collapsed": {
          "styles": {
